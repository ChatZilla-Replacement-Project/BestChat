<ComboBox xmlns="https://github.com/avaloniaui"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
					xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
					xmlns:data="using:BestChat.Platform.DataAndExt.Cmd.ParamTypes"
					mc:Ignorable="d"
					x:Class="BestChat.IRC.ProtocolMgr.Prefs.SpecializedCtrls.CmdParamSelComboBox">
	<ComboBox.ItemTemplate>
		<DataTemplate DataType="data:Abstract">
			<Label Content="{Binding LocalizedName}"
						ToolTip.Tip="{Binding LocalizedDesc}"/>
		</DataTemplate>
	</ComboBox.ItemTemplate>
</ComboBox>